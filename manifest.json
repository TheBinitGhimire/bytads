{
  "manifest_version": 2,
  "name": "bytads",
  "version": "0.0.1",
  "author": "Binit Ghimire",
  "description": "Bypass YouTube ads!",
  "icons": {
    "48": "icons/48.png",
    "96": "icons/96.png",
    "128": "icons/128.png",
    "256": "icons/256.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "bytads.js"
      ]
    }
  ]
}